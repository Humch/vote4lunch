{
  "name": "vote4lunch",
  "version": "0.2.0",
  "private": true,
  "main": "server.js",
  "proxy": "http://localhost:5000",
  "engines": {
    "node": "10.11.0"
  },
  "scripts": {
    "start": "node ./bin/start",
    "start-dev": "concurrently \"npm start\" \"npm run-script start-front\"",
    "test": "mocha --recursive ./test/*Test.js --compilers js:babel-core/register --require babel-polyfill",
    "heroku-postbuild": "npm run-script build-js",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm --max-old-space-size=8192 run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start-front": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "eject": "react-scripts eject",
    "import-data": "node ./node_modules/babel-cli/bin/babel-node.js commands/importData.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.4.0",
    "@fortawesome/free-regular-svg-icons": "^5.4.0",
    "@fortawesome/free-solid-svg-icons": "^5.4.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "cookie-parser": "^1.4.3",
    "debug": "^3.0.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.3",
    "leaflet": "^1.3.4",
    "material-icons-react": "^1.0.4",
    "moment": "^2.22.2",
    "morgan": "^1.8.2",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "pg": "^7.5.0",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dates": "^18.1.0",
    "react-dom": "^16.5.0",
    "react-leaflet": "^2.0.1",
    "react-leaflet-control": "^2.0.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "reactstrap": "^6.4.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0",
    "sequelize": "^4.39.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "concurrently": "^4.0.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.2.1"
  }
}
