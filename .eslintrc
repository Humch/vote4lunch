{
  "parser": "babel-eslint",
  "extends": [
    "plugin:react/recommended"
  ],
  "env": {
    "es6": true,
    "node": true,
    "mocha": true,
    "browser": true
  },
  "ecmaFeatures": {
    "classes": true,
    "modules": true
  },
  "rules": {
    "comma-dangle": 0,
    "linebreak-style": 0,
    "react/jsx-uses-react": 2
  },
  "plugins": [
    "react"
  ]
}